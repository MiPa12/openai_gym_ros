<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find turtlebot3_training)/config/turtlebot3_world_params.yaml" />
    <!-- Launch the training system -->
    <node pkg="turtlebot3_training" name="turtlebot3" type="start_training.py" output="screen"/>
</launch>
